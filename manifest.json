{

    "manifest_version": 2,
    "name": "Keanify",
    "description": "Replaces (most) images with breathtaking Keanu",
    "version": "0.1",
    "homepage_url": "https://github.com/caudatecoder/keanify",
    "icons": {
      "48": "icons/keanu-48x48.png",
      "96": "icons/keanu-96x96.png"
    },
  
    "browser_action": {
      "default_icon": "icons/keanu-32x32.png",
      "default_title": "Keanify"
    },  

    "permissions": [
      "activeTab",
      "contextMenus"
    ],
    
    "background": {
      "scripts": ["src/background.js"]
    }
}